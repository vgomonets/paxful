{
  "manifest_version": 2,
  "name": "open-my-page",
  "version": "1.0",
  "icons": {
    "48": "icons/page-48.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      },
      {
        "matches": ["*://*.paxful.com/*"],
        "js": ["jquery.js", "grabber-script.js"]
      }
    ]
}
